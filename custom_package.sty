\newcommand{\rank}[1]{\operatorname{rank}\pars*{#1}}
\newcommand{\matr}[1]{\brackets*{#1}}
\newcommand{\unit}{\mathbf{e}}
\newcommand{\basis}[1]{\operatorname{basis}\pars*{#1}}
\renewcommand{\dim}[1]{\operatorname{dim}\pars*{#1}}
\newcommand{\bvec}[1]{\mathbf #1}
\newcommand{\mat}[1]{\mathbf #1}
\newcommand{\identity}{\mathbf I}
\newcommand{\transpose}{\intercal}
\newcommand{\norm}[1]{\verts*{#1}}
\newcommand{\inp}[2]{\angles*{#1, #2}}
\newcommand{\tr}[1]{\operatorname{tr}\pars*{#1}}
% \newcommand{\adjoint}[1]{#1^\textbf{*}}
\newcommand{\adjoint}[1]{#1^\star}

% Logic
\newcommand{\quant}[1]{\left(#1\right)}

% Combinatorics
\newcommand{\inversion}[1]{\operatorname{inv}\pars*{#1}}
\newcommand{\runs}[1]{\operatorname{runs}\pars*{#1}}
