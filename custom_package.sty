% Set Properties
\newcommand{\power}[1]{\mathcal P\pars*{#1}}
\newcommand{\comp}[1]{#1^\complement}

% Sets
\newcommand{\reals}{\mathbb R}
\newcommand{\complex}{\mathbb C}
\newcommand{\naturals}{{\mathbb N}}
\newcommand{\algebraics}{{\mathbb K}}
\newcommand{\rationals}{\mathbb Q}
\newcommand{\integers}{\mathbb Z}
\newcommand{\funset}[2]{\mathcal{F}\pars*{#1, #2}}

% Special Set Symbols
\newcommand{\ii}{\mathfrak{i}}
\newcommand{\Nnaturals}{{\aleph_0}}
\newcommand{\Nreals}{{\mathfrak c}}

% Functions
\newcommand{\inv}[1]{{#1}^{-1}}
\newcommand{\domain}[1]{\operatorname{dom}\pars*{#1}}
\newcommand{\range}[1]{\operatorname{range}\pars*{#1}}

% Polynomials
\newcommand{\degree}[1]{\mathop{\mathrm{deg} \pars{#1}}}

% Calculus
\newcommand{\dif}[1]{\operatorname{d}\!#1 \mathop{}\!}
\newcommand{\Dt}[2][]{\dfrac{\mathrm{d} #1}{\mathrm{d}#2}}
\newcommand{\Dtn}[3][]{\dfrac{d^{#2}\!#1}{d #3^{#2}}}
\newcommand{\D}[2][]{\dfrac{\partial #1}{\partial #2}}
\newcommand{\curl}[1]{\nabla \times #1}
\newcommand{\divergence}[1]{\nabla \cdot #1}
\newcommand{\grad}[1]{\nabla #1}

% Linear Algebra
\newcommand{\linset}[2]{\operatorname{\mathcal{L}}\pars*{#1, #2}}
\newcommand{\zero}{\mathbf 0}
\newcommand{\zeromat}{\mathbf O}
\newcommand{\Span}[1]{\operatorname{span}\pars*{#1}}
\newcommand{\Null}[1]{\operatorname{null}\pars*{#1}}
